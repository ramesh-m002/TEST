public function getCourseOfferings($searchData)
{
    // searchData MUST be an array matching C# model
    $jsonBody = wp_json_encode($searchData);
    $response = wp_remote_post(
        $this->configJson->urls->courseOfferingURL,
        array(
            'method'  => 'POST',
            'timeout' => $this->timeout,
            'headers' => array(
                'Content-Type' => 'application/json',
                'Accept'       => 'application/json'
            ),
            'body'    => $jsonBody
        )
    );
    if (is_wp_error($response)) {
        error_log($response->get_error_message());
        return false;
    }
    return wp_remote_retrieve_body($response);
}
